@model IEnumerable<News.Entities.Submission>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.submission_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.submission_Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.submission_StartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.submission_DueTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.submission_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.submission_Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.submission_StartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.submission_DueTime)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.submission_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.submission_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.submission_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
